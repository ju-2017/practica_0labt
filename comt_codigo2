#include <iostream>

using namespace std;

int main() 
{
    int i = 1, j = 2; //  se asigana variables de tipo entero y su valor 
    
    if(i > j && j > i)// condicion si i mayor que j y j mayor que i 
        i++;           //si se cumple sumarle uno a la variable i
    if(i > j || j > i)// si i mayor que j o j mayor que i 
        j++;          // si se cumple sumarle 1 a la j
    if(i | j)         //si i o j estan
        i++;          // si se cumple sumarle 1 a la i 
    if(i & j)         // si i y j estan 
        j++;          // si se cumple sumarle 1 a la j 
    
    cout<<i * j<<endl; // se muestra la multiplicacion de i y j en pantalla esta es 8 la cual es la multiplicacion
          // de i que vale 2 y j que vale 4 los valores que toman despues de cumplirse las condiciones 2, 3 y 4 
    return 0;
}
